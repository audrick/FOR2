<h1>Users#list</h1>

<%= link_to "Edit Your Meal Preferences", edit_user_path %><br/>

<h1>List of meals based on preference</h1>
<br/>
<table>
  <tr>
    <th>Meal</th>
    <th>Zip Code</th>
    <th>Seller</th>
    <th>Price</th>
    <th></th>
  </tr>

<% @list.each do |meal| %>
  <tr>
    <td><%= link_to meal.meal_name, meal_path(meal) %></td>
    <td><%= meal.users.first.zip_code %></td>
    <td><%= meal.users.first.username %></td>
    <td><p>$<%= meal.meal_price %></p></td>
  </tr>
<% end %>
</table>

<br/>
<br/>
<%= link_to "Edit Your Meal Preferences", edit_user_path %><br/><br/>




